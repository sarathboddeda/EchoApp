<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include android:id="@+id/del_mer_order"
        layout="@layout/header"></include>
    <RelativeLayout
        android:id="@+id/rl_delmerordrr"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="30dp"
        android:layout_below="@+id/del_mer_order"
        android:layout_marginTop="10dp">

        <TextView
            android:id="@+id/tv_delmerordrtitle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-condensed"
            android:text="Merchant Orders"
            android:textStyle="bold"
            android:gravity="center"
            android:textSize="25dp"
            android:textColor="#1684bf"
            android:layout_marginBottom="10dp"/>
        <ListView
            android:id="@+id/lv_del_merchant_orders"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_delmerordrtitle"></ListView>
    </RelativeLayout>


</RelativeLayout>